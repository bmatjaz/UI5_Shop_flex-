<mvc:View
	controllerName="sap.ui.flex.shop.controller.Checkout"
 	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="page"
		title="{i18n>productsInCart}"
		backgroundDesign="Solid"
		showNavButton="{appView>/smallScreenMode}"
		navButtonPress=".onBack"
		showFooter="true">
        <Button text="knof" press="backToShop" />
	</Page>
</mvc:View>