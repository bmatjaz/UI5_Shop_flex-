{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.flex.shop",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "http://localhost:8081/https://services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.flex.shop.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.uxap": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.flex.shop.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "mainService"
			}
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.flex.shop.view",
				"controlId": "flexibleColumnLayout",
				"transition": "slide",
				"bypassed": {
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "master",
					"target": [
						"master"
					],
					"layout": "OneColumn"
				},
				{
					"pattern": "products/{categoryID}",
					"name": "products",
					"target": [
						"master",
						"products"
					],
					"layout": "TwoColumnsMidExpanded"
				},
				{
					"pattern": "products/{categoryID}/cart",
					"name": "cart",
					"target": [
						"master",
						"products",
						"cart"
					],
					"layout": "ThreeColumnsEndExpanded"
				},
				{
					"pattern": "checkout",
					"name": "checkout",
					"target": [
						"checkout"
					],
					"layout": "OneColumn"
				},
				{
					"pattern": "orderConfirmation",
					"name": "orderConfirmation",
					"target": [
						"master", "orderConfirmation"
					],
					"layout": "TwoColumnsMidExpanded"
				}

			],
			"targets": {
				"master": {
					"viewName": "Master",
					"controlAggregation": "beginColumnPages"
				},
				"products": {
					"viewName": "Products",
					"controlAggregation": "midColumnPages"
				},
				"cart": {
					"viewName": "Cart",
					"controlAggregation": "endColumnPages"
				},
				"checkout": {
					"viewName": "Checkout",
					"controlAggregation": "beginColumnPages"
				},
				"orderConfirmation": {
					"viewName": "OrderConfirmation",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	}
}